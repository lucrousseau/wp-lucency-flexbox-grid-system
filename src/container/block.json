{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "lucency-flexbox-grid-system/container",
	"version": "0.1.0",
	"title": "Lucency Container",
	"category": "design",
	"description": "Create a container to add all other components.",
	"example": {
		"innerBlocks": [
			{
				"name": "core/heading",
				"attributes": {
					"level": 1,
					"content": "Title"
				}
			},
			{
				"name": "lucency-flexbox-grid-system/row",
				"innerBlocks": [
					{
						"name": "lucency-flexbox-grid-system/column",
						"innerBlocks": [
							{
								"name": "core/paragraph",
								"attributes": {
									"content": "Lorem ipsum dolor sit amet..."
								}
							}
						]
					},
					{
						"name": "lucency-flexbox-grid-system/column",
						"innerBlocks": [
							{
								"name": "core/paragraph",
								"attributes": {
									"content": "Lorem ipsum dolor sit amet..."
								}
							}
						]
					}
				]
			}
		]
	},
	"supports": {},
	"parent": [null],
	"textdomain": "lucency",
	"author": "Luc Rousseau",
	"authorURI": "https://lucrousseau.com",
	"license": "GPL v2 or later",
	"licenseURI": "https://www.gnu.org/licenses/gpl-2.0.html",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"viewScript": "file:./view.js",
	"keywords": [
		"column",
		"col",
		"cols",
		"flex",
		"flexbox",
		"grid",
		"row",
		"container",
		"lucency",
		"responsive",
		"layout"
	],
	"attributes": {
		"className": {
			"type": "string",
			"default": ""
		},
		"style": {
			"type": "object",
			"default": {}
		},
		"stylesClasses": {
			"type": "object",
			"default": {
				"full": {
					"classes": {},
					"variables": {
						"padding-top": { "value": 3, "unit": "rem" },
						"padding-bottom": { "value": 3, "unit": "rem" },
						"padding-left": { "value": 2, "unit": "rem" },
						"padding-right": { "value": 2, "unit": "rem" }
					}
				}
			}
		}
	}
}
